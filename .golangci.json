{
	"linters": {
		"disable-all": true,
		"enable": [
			"govet",
			"revive",
			"goimports",
			"misspell",
			"ineffassign",
			"gofmt"
		]
	},
	"run": {
            "timeout": "10m"
	},
	"issues": {
		"exclude-rules": [
			{
				"linters": ["revive"],
				"text": "should have comment or be unexported"
			}
		],
		"exclude-dirs": [
			"pkg/generated"
		]
	}
}
